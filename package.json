  },
  "homepage": "https://github.com/ova2/webworkers-pool#readme",
  "devDependencies": {
    "del-cli": "~1.1.0",
    "parcel-bundler": "~1.12.3",
    "tslint": "~5.10.0",
    "typescript": "~3.4.2"
  },
  "dependencies": {},
  "files": [
    "dist/index.js",
    "dist/index.d.ts"
  ]
