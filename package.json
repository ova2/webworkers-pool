{
  "name": "webworkers-pool",
  "version": "1.0.0",
  "description": "Pool implementation for Web Workers threads",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "start": "parcel showcase/index.html --open",
    "clean:dist": "del-cli dist",
